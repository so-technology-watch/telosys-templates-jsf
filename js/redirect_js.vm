function gotoForeignKeyPage(urlId, listId, keyName) {
	var eUrlId = document.getElementById(urlId);
	if (eUrlId != null) {
		var destination = eUrlId.getAttribute("href");
		var eListId = document.getElementById(listId);
		if (eListId != null
				&& eListId.options[eListId.selectedIndex].value != "") {
			destination += "?" + keyName + "="
					+ eListId.options[eListId.selectedIndex].value
		}
		// alert("destination : " + destination);
		window.location.href = destination;
	}
}